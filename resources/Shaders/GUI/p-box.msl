#version 1.0
#type PIXEL

/*
	GUI box element pixel shader.
*/

Output out
{
	float4 color : ATTACHMENT_0;
};

ConstantBuffer boxData : BOX_DATA
{
	float44 transform;
	float4 backgroundColor;
	float2 boxSize;
};

Shader
{
	out.color = boxData.backgroundColor;
	out.color.r = 1.0f;
	out.color.a = 1.0f;
};
